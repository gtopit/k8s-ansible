kind: KubeProxyConfiguration
apiVersion: kubeproxy.config.k8s.io/v1alpha1
address: 0.0.0.0
metricsBindAddress: {{ inventory_hostname }}:10249
clientConnection:
  kubeconfig: {{ k8s_work_dir }}/conf/kube-proxy.kubeconfig
hostnameOverride: {{ node_hostname }} 
clusterCIDR: {{ pod_cidr }}
mode: ipvs
ipvs:
  scheduler: "rr"
iptables:
  masqueradeAll: false